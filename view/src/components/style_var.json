{
    "colorBase": {
        "yellow": "#FFE70B",
        "black": "#454545",
        "white": "#ffffff",
        "whiteO": "rgba(255,255,255,0.75)",
        "greyL1": "#F1F3F4",
        "greyL2": "#DDE2E5",
        "red": "#D0021B"
    },
    "shadow": "0 1px 2px 0 rgba(0,0,0,0.1)",
    "ctashadow": "0 1px 16px 0px rgba(0,0,0,0.1)",
    "textshadow": "0 1px 1px rgba(0,0,0,0.25)",
    "font": {
        "family": "'PingFang SC', 'SF Pro Display', 'SF UI Display', Helvetica, Arial",
        "size": {
            "s": "10px",
            "m": "18px",
            "l": "24px"
        },
        "weight": {
            "light": 300,
            "regular": 400,
            "medium": 600,
            "bold": 700
        }
    },
    "spacing": {
        "unit": 8
    },
    "transition": {
        "fast": "0.1s ease-in-out",
        "normal": "0.2s ease-in-out",
        "slow": "0.5s ease-in-out",
        "spring": "0.2s cubic-bezier(0.175, 0.885, 0.32, 1.275)"
    },
    "animation": {
        "shake": "{0%{transform:translateX(0);}25%{transform:translateX(-8px);}50%{transform:translateX(0);}75%{transform: translateX(8px);}100%{transform:translateX(0);}}",
        "fadein": "{from{opacity:0}to{opacity:1}"
    }
}
